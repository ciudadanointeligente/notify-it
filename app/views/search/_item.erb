<li class="item <%= item.subscription.subscription_type %>" 
  data-subscription_type="<%= item.subscription.subscription_type %>" 
  data-search_type="<%= interest.search_type %>">
  
  <time class="result" datetime="<%= html_date item.date %>">
    <%= just_date_year item.date %>
  </time>
  
  <%= partial "subscriptions/#{item.subscription.subscription_type}/result", engine: "erb", locals: {interest: interest, item: item, query: query} %>
</li>
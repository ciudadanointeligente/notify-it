<% items.each do |item| %>
  <%
  unless defined?(interest)
    interest = item.interest
  end
  %>

  <item>

    <%= partial "subscriptions/#{item.subscription_type}/rss", engine: "erb", locals: {item: item, interest: interest} %>

    <link><%= item_url item %></link>
    <guid><%= item_guid item %></guid>
    <pubDate><%= rss_date item.date %></pubDate>
  </item>
<% end %>